{
  "name": "docauto",
  "version": "1.0.0",
  "description": "Convert docx/odt files to PDF, and merge it",
  "main": "dist/index.mjs",
  "scripts": {
    "lint": "eslint \"src/**/*\"",
    "clean": "rimraf dist",
    "build": "esbuild src/index.ts --outdir=dist --target=node16 --platform=node --format=cjs --bundle --minify"
  },
  "keywords": [
    "pdfmerger",
    "ilovepdf",
    "docs2pdf",
    "odt2pdf"
  ],
  "author": "Hanif Dwy Putra S <hanifdwyputrasembiring@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.8.2",
    "@typescript-eslint/eslint-plugin": ">=5.31.0",
    "@typescript-eslint/parser": ">=5.31.0",
    "esbuild": "^0.15.10",
    "eslint": ">=8.0.0",
    "eslint-config-xo": "^0.42.0",
    "eslint-config-xo-typescript": "^0.53.0",
    "rimraf": "^3.0.2",
    "typescript": ">=4.4"
  },
  "dependencies": {
    "actions-toolkit": "^6.0.1",
    "ilovepdf-freedom": "^1.1.0"
  },
  "packageManager": "yarn@3.2.4"
}
